{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting":[ {
    "public": "dist/batiment-dirif/browser",
    "target": "dev",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
       "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
{
    "public": "dist/batiment-dirif/browser",
    "target": "prod",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
       "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
]
}


